<view>
  <image class="logo" src="../../static/image/猪头像.png"></image>
  <image mode="aspectFit" class="title" src="../../static/image/title.png"></image>
  <form bindsubmit="formSubmit">
    <view class="m-login">
      <input class="login-inp" name='countName' focus="true" placeholder-class="inp-place" type="number" maxlength="20"  placeholder="请输入账号" />
      <input class="login-inp" name='password' placeholder-class="inp-place" password="true"  placeholder="密码" />
      <input class="login-inp" name='userName' focus="true" placeholder-class="inp-place" type="number" maxlength="20"  placeholder="请输入姓名" />
      <input class="login-inp" name='phone' focus="true" placeholder-class="inp-place" type="number" maxlength="20"  placeholder="请输入电话" />
      <input class="login-inp" name='brand' focus="true" placeholder-class="inp-place" type="number" maxlength="20"  placeholder="请输入养殖厂信息" />
      
      
      <button class="login-btn" loading="{{load}}" formType="submit">注册</button>
    </view>
  </form>
</view>